    <changeSet author="batizhao" id="CreateTable-20210610-001">
        <createTable tableName="comment" remarks="审批表">
            <column autoIncrement="true" name="id" type="BIGINT" remarks="主键">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="title" type="VARCHAR(32)" remarks="标题" defaultValue="">
                <constraints nullable="false"/>
            </column>
            <column name="comment" type="VARCHAR(255)" remarks="意见" defaultValue=""/>
            <column name="dept" type="VARCHAR(64)" remarks="发单部门" defaultValue="">
                <constraints nullable="false"/>
            </column>
            <column name="date" type="datetime" remarks="发单日期" defaultValueComputed="NOW()">
                <constraints nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(64)" remarks="项目名称" defaultValue="">
                <constraints nullable="false"/>
            </column>
            <column name="investment" type="DECIMAL(10,2)" remarks="投资金额" defaultValue="0.00">
