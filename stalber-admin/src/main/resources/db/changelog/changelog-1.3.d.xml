<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.2.xsd">

    <changeSet author="batizhao" id="CreateColumnData-20220222-001">
        <insert tableName="dict_type">
            <column name="id" valueNumeric="6"/>
            <column name="name" value="字段类型"/>
            <column name="code" value="db_column_type"/>
            <column name="description" value="VARCHAR|INT"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="11"/>
            <column name="code" value="db_column_type"/>
            <column name="label" value="字符串"/>
            <column name="value" value="VARCHAR"/>
            <column name="sort" valueNumeric="1"/>
            <column name="isDefault" value="yes"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="12"/>
            <column name="code" value="db_column_type"/>
            <column name="label" value="数字"/>
            <column name="value" value="INT"/>
            <column name="sort" valueNumeric="2"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="13"/>
            <column name="code" value="db_column_type"/>
            <column name="label" value="日期"/>
            <column name="value" value="DATETIME"/>
            <column name="sort" valueNumeric="3"/>
        </insert>
    </changeSet>

    <changeSet author="batizhao" id="CreateTableStatusData-20220222-002">
        <insert tableName="dict_type">
            <column name="id" valueNumeric="7"/>
            <column name="name" value="表同步状态"/>
            <column name="code" value="table_sync_status"/>
            <column name="description" value="created|nosync|synced"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="20"/>
            <column name="code" value="table_sync_status"/>
            <column name="label" value="已创建"/>
            <column name="value" value="created"/>
            <column name="sort" valueNumeric="1"/>
            <column name="isDefault" value="yes"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="21"/>
            <column name="code" value="table_sync_status"/>
            <column name="label" value="未同步"/>
            <column name="value" value="nosync"/>
            <column name="sort" valueNumeric="2"/>
        </insert>
        <insert tableName="dict_data">
            <column name="id" valueNumeric="22"/>
            <column name="code" value="table_sync_status"/>
            <column name="label" value="已同步"/>
            <column name="value" value="synced"/>
            <column name="sort" valueNumeric="3"/>
        </insert>
    </changeSet>

</databaseChangeLog>